 class Prime_Number
{
	public static void main(String[] args)// let see result
	{
	    int num = 7; // we are checking given number is prime number or not a prime number
	    
	    int count =0;

	    for (int i=1;i<=227; i++)// we are finding factors of 227 by using loop 
	    {
	        if (num%i==0)		// Ex:- 7 % 1 = reminder is 0 so 1 is a factor of 7  
	        {
	            count++;
	        }
	        
	    }
	    
	    if(count==2)//prime number having two factors 
	    {
	        System.out.println("Given number is a prime number"+num);
	    }
	    else
	    {
	         System.out.println("Given number is  not  a prime number"+num);
	        
	    }
	}
	
}